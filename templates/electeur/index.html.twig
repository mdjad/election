{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block content %}
    <div class="container-fluid mt-3">
        <div id='info' class="row clearfix ">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="card-header">
                        <h2>Inscription à la liste electorale</h2>
                    </div>
                    <div class="card-body">
                        <h4 class="card-title">
                            <i class="material-icons md-24">
                                information
                            </i>
                        </h4>
                        <p class="card-text"> Avant de commencer l'inscription munissez-vous de votre carte d'identité en format numerique.</p>
                        <div class="text-center">
                            <button type="button" class="btn btn-primary"  onclick="document.location.href='{{url('inscription_new')  }}'">Commencer l'inscription </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}



