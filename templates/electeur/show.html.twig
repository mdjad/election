{% extends 'base.html.twig' %}

{% block title %}Electeur{% endblock %}

{% block body %}
    <h1>Electeur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ electeur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ electeur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ electeur.prenom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ electeur.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ electeur.email }}</td>
            </tr>
            <tr>
                <th>Num_carte</th>
                <td>{{ electeur.numCarte }}</td>
            </tr>
            <tr>
                <th>Cni_photo</th>
                <td>{{ electeur.cniPhoto }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ electeur.updatedAt ? electeur.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('electeur_index') }}">back to list</a>

    <a href="{{ path('electeur_edit', {'id': electeur.id}) }}">edit</a>

    {{ include('electeur/_delete_form.html.twig') }}
{% endblock %}
